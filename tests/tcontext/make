CONTEXT_HDRS=..\..\hdrs\context.h
SCONTEXT_HDRS= scontext.h
TCONTEXT_HDRS=..\..\hdrs\context.h ints.h scontext.h tcontext.h


tcontext.com: tcontext.obj scontext.obj ..\..\objs\context.obj
	wcl -0 -lr -l=COM tcontext.obj scontext.obj ..\..\objs\context.obj
	
tcontext.obj: tcontext.c $(TCONTEXT_HDRS)
	wcc -0 -ox tcontext.c
	
scontext.obj: scontext.c $(SCONTEXT_HDRS)
	wcc -0 -ox scontext.c
	
..\..\objs\context.obj: ..\..\src\context.c $(CONTEXT_HDRS)
	wcc -0 ..\..\src\context.c -fo=..\..\objs\context.obj -fr=..\..\errs\context.err